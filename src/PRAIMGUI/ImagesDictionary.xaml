<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="Icon16x16" TargetType="{x:Type Image}">
        <Setter Property="Width" Value="16"/>
        <Setter Property="Height" Value="16"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
    </Style>

    <BitmapImage x:Key="CameraIcon" UriSource="Images/camera-photo.png"/>
    <BitmapImage x:Key="DBIcon" UriSource="Images/system-file-manager.png"/>
    <BitmapImage x:Key="DocumentSave" UriSource="Images/document-save.png"/>
    <BitmapImage x:Key="CancelIcon" UriSource="Images/emblem-unreadable.png"/>
    <BitmapImage x:Key="SaveIcon" UriSource="Images/media-floppy.png"/>
    <BitmapImage x:Key="ProjectsIcon" UriSource="Images/preferences-system-windows.png"/>
    <BitmapImage x:Key="ListAddIcon" UriSource="Images/list-add.png"/>
    <BitmapImage x:Key="ListRemoveIcon" UriSource="Images/list-remove.png"/>
    
</ResourceDictionary>