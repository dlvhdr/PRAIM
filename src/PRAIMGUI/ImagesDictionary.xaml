<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="Icon16x16" TargetType="{x:Type Image}">
        <Setter Property="Width" Value="16"/>
        <Setter Property="Height" Value="16"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
    </Style>

    <BitmapImage x:Key="MetroCamera" UriSource="Images/Windows-Metro/Camera.png"/>
    <BitmapImage x:Key="MetroCancel" UriSource="Images/Windows-Metro/Cancel.png"/>
    <BitmapImage x:Key="MetroSaveImage" UriSource="Images/Windows-Metro/Save pictures.png"/>
    <BitmapImage x:Key="MetroOK" UriSource="Images/Windows-Metro/OK.png"/>
    <BitmapImage x:Key="MetroSaveImageNoCircle" UriSource="Images/Windows-Metro/Save pictures_no_circle.png"/>
    <BitmapImage x:Key="MetroSave" UriSource="Images/Windows-Metro/Save.png"/>
    <BitmapImage x:Key="MetroSaveCircle" UriSource="Images/Windows-Metro/Save_circle.png"/>
    <BitmapImage x:Key="MetroErase" UriSource="Images/Windows-Metro/Erase_black.png"/>

    <BitmapImage x:Key="AppIconWhite" UriSource="Images/rsz_praim_white.png"/>
    <BitmapImage x:Key="AppBlackICO" UriSource="Images/rsz_praim_black.ico"/>

</ResourceDictionary>